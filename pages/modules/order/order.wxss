page {
    background-color: #f1f1f1;
}

.order-tags {
    padding: 0 30px;
    background: #fff;
}

.ui-order-tag {
    font-size: 16px !important;
}

.ui-order-tags {
    line-height: 50px !important;
    height: 50px !important;
}

.order-list {
    transition: opacity ease-out 500ms;
}

.order-list .ui-row-border-top:before
/*:first-child:before*/

{
    display: none;
}

.order-button {
    margin: 0 !important;
    height: 37px !important;
    line-height: 35px !important;
}

.i-swipeout-image image {
    width: 48px;
    height: 48px;
}

.order-list {
    margin: 10px 0;
}

.order-list .ui-row-border-top {
    padding: 20px;
    min-height: 100px;
    border-radius: 8px;
    background-color: #fff;
}

.ui-input-title {
    padding-right: 25px !important;
}

.ui-last-input::after {
    display: none;
}

.nav_top_view {
    width: 100%;
    position: relative;
}

.nav_top_view view {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}