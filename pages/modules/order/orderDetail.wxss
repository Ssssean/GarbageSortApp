page {
    background-color: #F6F6F6;
}

.ui-steps {
    position: relative;
}

.ui-steps.dashed {
    border-bottom: none;
}

.ui-top-pop {
    color: #495060;
    padding: 10px;
    margin-top: 10px;
    position: relative;
}

.ui-row+.ui-row {
    margin-top: 5px;
}

.ui-title {
    color: #333;
    font-weight: 300;
    font-size: 14px;
    margin: 8px 0;
    padding-left: 10px;
    border-left: 2px solid #308FDB;
}

.ui-col-8 {
    padding-left: 8px;
}

.goods-box {
    padding-top: 0;
}

.title-box {
    margin-top: 0;
    padding-top: 10%;
    margin-bottom: 0;
    padding-bottom: 15%;
    background-color: #fff;
}

.progress-content,
.progress-tags {
    margin-top: 10px;
}

.progress-tags+.progress-tags {
    margin-top: 5px;
}

.progress-tags .label {
    display: inline-block;
}